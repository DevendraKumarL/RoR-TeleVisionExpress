<br><br><br><br><br><br><br><br>

<div class="container">
    <div class="row">
        <table class="table table-striped table-bordered table-hover table-responsive">
            <thead>
                <th>id</th>
                <th>season_name</th>
                <th>tvshows_id</th>
                <th>year</th>
                <th>season_overview</th>
                <th>rating</th>
                <th>image</th>
                <th>language</th>
                <th>no_episodes</th>
            </thead>
            <tbody>
                 <% @seasons.each do |season| %>
                    <tr>
                        <td><%= season.id %></td>
                        <td><%= season.season_name %></td>
                        <td><%= season.tvshows_id %></td>
                        <td><%= season.year %></td>
                        <td>
                            <dl class="dl-horizontal">
                                <dt class="text-truncate">
                                    <%= season.season_overview %>
                                </dt>
                            </dl>
                        </td>
                        <td><%= season.rating %></td>
                        <td><%= season.image %></td>
                        <td><%= season.language %></td>
                        <td><%= season.no_episodes %></td>
                    </tr>
                <% end %>
            </tbody>
        </table>
    </div>
</div>