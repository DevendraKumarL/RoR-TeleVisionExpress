    <div class="season">
        <% @tvshow_name.each do |show| %>
        <h2 style="padding-top: 10px;padding-left: 10px;"><%= show.name %></h2>
        <% end %>
        <% @this_season.each do |season| %>
        <div class="season_overview">
            <a href="<%= a_episode_path(season.id) %>"><b><%= season.season_name %></b>(<%= season.year %>) - <%= season.no_episodes %> Episodes</a><br>
            <img src="<%= season.image %>" class="season_image">
            <p>
                <%= season.season_overview %>
            </p>
        </div>
        <% end %><br>
    </div>